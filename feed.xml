---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Fang Releases</title>
    <description>Release notes for Fang, an RSS feed reader.</description>
    <link>https://getfang.com/feed.xml/</link>
    <atom:link href="https://getfang.com/feed.xml" rel="self" type="application/rss+xml"/>
    {% for release in site.data.releases %}
    <item>
      <title>Fang {{ release.version }}</title>
      <link>https://github.com/MrEricSir/Fang/releases/tag/{{ release.tag }}</link>
      <guid isPermaLink="true">https://github.com/MrEricSir/Fang/releases/tag/{{ release.tag }}</guid>
      <pubDate>{{ release.date | date_to_rfc822 }}</pubDate>
      <description><![CDATA[
        <h2>Changelog</h2>
        <ul>
          {% for item in release.changelog %}
          <li>{{ item.text }}</li>
          {% endfor %}
        </ul>
        <h2>Downloads</h2>
        <ul>
          <li><a href="https://github.com/MrEricSir/Fang/releases/download/{{ release.tag }}/Fang.msi">Windows</a></li>
          <li><a href="https://github.com/MrEricSir/Fang/releases/download/{{ release.tag }}/Fang.dmg">macOS</a></li>
          <li><a href="https://github.com/MrEricSir/Fang/releases/download/{{ release.tag }}/Fang.AppImage">Linux (AppImage)</a></li>
          <li><a href="https://github.com/MrEricSir/Fang/archive/refs/tags/{{ release.tag }}.zip">Source code</a></li>
        </ul>
      ]]></description>
    </item>
    {% endfor %}
  </channel>
</rss>
